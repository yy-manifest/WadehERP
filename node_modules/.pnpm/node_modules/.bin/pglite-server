#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/pglite-socket/dist/scripts/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/pglite-socket/dist/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/pglite-socket/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/pglite-socket/dist/scripts/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/pglite-socket/dist/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/pglite-socket/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules/@electric-sql/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/@electric-sql+pglite-socket@0.0.6_@electric-sql+pglite@0.3.2/node_modules:/Users/ysn/wadeherp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@electric-sql/pglite-socket/dist/scripts/server.js" "$@"
else
  exec node  "$basedir/../@electric-sql/pglite-socket/dist/scripts/server.js" "$@"
fi
